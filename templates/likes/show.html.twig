<!DOCTYPE html>

<title>Likes</title>

{% block body %}
    <h1>Likes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ like.id }}</td>
            </tr>
            <tr>
                <th>UserId</th>
                <td>{{ like.userId }}</td>
            </tr>
            <tr>
                <th>PosteId</th>
