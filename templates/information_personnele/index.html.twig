                <td>{{ information_personnele.numTel }}</td>
                <td>{{ information_personnele.adresse }}</td>
                <td>
                    <a href="{{ path('app_information_personnele_show', {'id': information_personnele.id}) }}">Show</a>

                    <a href="{{ path('app_information_personnele_edit', {'id': information_personnele.id}) }}">edit</a>
