<!DOCTYPE html>

<title>InformationPersonnele index</title>

{% block body %}
    <h1>InformationPersonnele index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Sexe</th>
                <th>Taille</th>
                <th>Poids</th>
                <th>Maladie</th>
                <th>NumTel</th>
                <th>Adresse</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for information_personnele in information_personneles %}
