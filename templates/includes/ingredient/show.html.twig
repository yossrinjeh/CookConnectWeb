{% extends 'base.html.twig' %}

{% block title %}Ingredient{% endblock %}

{% block body %}
    <h1>Ingredient</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ingredient.id }}</td>
            </tr>
            <tr>
                <th>UserId</th>
                <td>{{ ingredient.userId }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ ingredient.nom }}</td>
            </tr>
            <tr>
                <th>Qte</th>
                <td>{{ ingredient.qte }}</td>
            </tr>
            <tr>
                <th>PrixVente</th>
                <td>{{ ingredient.prixVente }}</td>
            </tr>
            <tr>
                <th>PrixAchat</th>
                <td>{{ ingredient.prixAchat }}</td>
            </tr>
            <tr>
                <th>IdNutrition</th>
                <td>{{ ingredient.idNutrition }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ ingredient.etat }}</td>
            </tr>
            <tr>
                <th>QuantiteThreshold</th>
                <td>{{ ingredient.quantiteThreshold }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ ingredient.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ingredient_index') }}">back to list</a>
    <br>

    <a href="{{ path('app_ingredient_edit', {'id': ingredient.id}) }}">edit</a>

    {{ include('ingredient/_delete_form.html.twig') }}
{% endblock %}
